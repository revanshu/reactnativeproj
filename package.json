{
  "name": "gifmodule",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@gif/gifcomponents": "^0.49.0",
    "@gif/gifuicommon": "^2.10.1",
    "axios": "^0.18.0",
    "i18next": "^15.0.8",
    "moment": "^2.24.0",
    "react": "^16.4.1",
    "react-native": "^0.56.0",
    "react-native-vector-icons": "^6.4.2",
    "react-navigation": "^2.4.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/runtime": "7.4.2",
    "babel-jest": "24.6.0",
    "jest": "^24.6.0",
    "metro-react-native-babel-preset": "0.53.1",
    "react-test-renderer": "16.8.3",
    "jest-serializer-enzyme": "^1.0.0",
    "jest-sonar-reporter": "^2.0.0"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "window": {}
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^Common(.*)$": "<rootDir>/gifmodule/gifmodulecommon$1",
      "^Mobile(.*)$": "<rootDir>/gifmodule/gifmodulemobile$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/gifmodule/**.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base)/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "watchman watch-del-all && react-native start --reset-cache",
    "lint": "eslint gifprelabel",
    "validate": "npm run lint && npm run test",
    "snapshot": "npm publish"
  },
  "jestSonar": {
    "sonar56x": true
  }
}
